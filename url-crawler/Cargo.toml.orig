[package]
name = "url-crawler"
version = "0.3.0"
description = "Configurable parallel web crawler, designed to crawl for content"
repository = "https://github.com/pop-os/url-crawler"
authors = ["Michael Aaron Murphy <mmstickman@gmail.com>"]
license = "MIT"
readme = "README.md"
keywords = ["url", "web", "crawler"]
categories = ["web-programming"]

[dependencies]
url-scraper = "0.1.0"
reqwest = "0.9.2"
chrono = "0.4.6"
crossbeam-channel = "0.2.6"
bitflags = "1.0.4"
